{
    "name": "logy/microsoft-bundle",
    "type": "bundle",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "Logy\\Bundle\\MicrosoftBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lucas Napik",
            "email": "lucas.napik@logy.se"
        }
    ],
    "require": {
        "symfony/http-client": "*",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "jms/serializer": "*",
        "symfony/http-foundation": "*",
        "symfony/routing": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
